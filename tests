            ["Disco Bee"] = true, ["Raccoon"] = true, ["Dragonfly"] = true, ["Mimic Octopus"] = true,
            ["Butterfly"] = true, ["Queen Bee"] = true, ["T-Rex"] = true, ["Fennec Fox"] = true,
